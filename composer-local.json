{
	"name": "greenpeace/planet4-test-pandora",
	"description": "Greenpeace P4 test-pandora",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-master-theme": "7",
		"greenpeace/planet4-plugin-gutenberg-blocks": "dev-master",
		"plugins/sitepress-multilingual-cms": "*",
		"plugins/wpml-translation-management": "*",
		"plugins/wpml-string-translation": "*",
		"plugins/wpml-media-translation": "*"
	},
	"scripts": {
		"core:language": "wp language core install fr_CA",
		"site:custom": [
			"@core:language"
		]
	},
	"data_from": "africa",
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "greenpeace/planet4-master-theme",
				"type": "wordpress-theme",
				"autoload": {
					"psr-4": {
						"P4\\MasterTheme\\": "src/"
					}
				},
				"extra": {
					"installer-paths": {
						"vendor/plugins/{$name}/": [
							"type:wordpress-plugin"
						]
					}
				},
				"version": 7,
				"dist": {
					"type": "zip",
					"url": "https://storage.googleapis.com/planet4-packages/planet4-master-theme/ba655b7253ca578ec4a320e8356e51211f90c20b.zip"
				}
			}
		}
	]
}
