{
	"name": "greenpeace/planet4-test-pandora",
	"description": "Greenpeace P4 test-pandora",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-master-theme": "7",
		"greenpeace/planet4-plugin-gutenberg-blocks": "dev-master"
	},
	"scripts": {
		"install:plugin-apm": "wp plugin install --activate https://storage.googleapis.com/p4-ray-test/wordpress-apm-agent-elasticsearch.zip",
		"core:language": "wp language core install nl_NL; wp language core activate nl_NL",
		"site:custom": [
			"@install:plugin-apm",
			"@core:language"
		]
	},
	"data_from": "international",
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "greenpeace/planet4-master-theme",
				"type": "wordpress-theme",
				"autoload": {
					"psr-4": {
						"P4\\MasterTheme\\": "src/"
					}
				},
				"extra": {
					"installer-paths": {
						"vendor/plugins/{$name}/": [
							"type:wordpress-plugin"
						]
					}
				},
				"version": 7,
				"dist": {
					"type": "zip",
					"url": "https://storage.googleapis.com/planet4-packages/planet4-master-theme/be1c08756d8074b0af38a803c98e9426b8c50fe3.zip"
				}
			}
		}
	]
}
